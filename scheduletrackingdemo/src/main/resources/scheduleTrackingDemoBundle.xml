<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:osgi="http://www.springframework.org/schema/osgi"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/osgi http://www.springframework.org/schema/osgi/spring-osgi.xsd">

	<import resource="scheduleTrackingDemo.xml" />

	<osgi:reference cardinality="0..1" id="ivrServiceOsgi"
		interface="org.motechproject.ivr.service.IVRService" />

	<osgi:reference cardinality="0..1" id="cmsLiteService"
		interface="org.motechproject.cmslite.api.service.CMSLiteService" />

	<osgi:reference cardinality="0..1" id="scheduleTrackingServiceOsgi"
		interface="org.motechproject.scheduletracking.api.service.ScheduleTrackingService" />

	<osgi:reference cardinality="0..1" id="voiceOutboxServiceOsgi"
		interface="org.motechproject.outbox.api.service.VoiceOutboxService" />
		
	<osgi:reference cardinality="0..1" id="smsServiceOsgi" 
        interface="org.motechproject.sms.api.service.SmsService"/>

</beans>
